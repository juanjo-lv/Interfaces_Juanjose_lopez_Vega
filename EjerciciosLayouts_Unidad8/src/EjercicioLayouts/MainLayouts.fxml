                  <Rectangle arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="40.0" stroke="BLACK" strokeType="INSIDE" width="40.0" StackPane.alignment="CENTER_RIGHT" />
                  <Text fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-font-weight: bold;" text="?" StackPane.alignment="CENTER_RIGHT">
                     <font>
                        <Font size="30.0" />
